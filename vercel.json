{
  "buildCommand": "",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/c/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/editar/:id",
      "destination": "/public/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}
